{
	"name": "aws-cdk-dependencies-action",
	"version": "0.0.0",
	"description": "GitHub Action that retrieves the Node.js packages from an AWS CDK release by tag",
	"module": "src/index.ts",
	"author": "C. Spencer Beggs <spencer@beg.gs>",
	"repository": {
		"url": "https://github.com/spencerbeggs/aws-cdk-get-release-action.git",
		"type": "git"
	},
	"license": "MIT",
	"scripts": {
		"action": "yarn build && act",
		"clean": "rm -rf dist",
		"debug": "node --inspect dist/index.js",
		"docker-debug": "docker exec --it aws-cdk-dependencies-action-debug /bin/bash",
		"test": "NODE_ENV=production APP_ENV=jest jest",
		"bootstrap": "ts-node lib/bootstrap.ts init",
		"build": "rollup",
		"lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx,.json}' --quiet --fix --ignore-path .eslintignore",
		"package": "ts-node lib/package.ts",
		"type-check": "tsc -p build.json --noEmit",
		"dev": "yarn clean && NODE_ENV=development=local rollup -c rollup.config.ts --watch --waitForBundleInput",
		"rollup": "rollup -c rollup.config.ts",
		"github-schema": "apollo client:download-schema --config=apollo.config.js vendor/github.graphql",
		"codegen": "apollo client:codegen --config=apollo.config.js --target=typescript generated"
	},
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.11.4",
		"@babel/eslint-parser": "^7.11.4",
		"@babel/eslint-plugin": "^7.11.3",
		"@babel/node": "^7.10.5",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.11.0",
		"@babel/plugin-proposal-optional-chaining": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-typescript": "^7.10.4",
		"@octokit/graphql": "^4.5.4",
		"@octokit/graphql-schema": "^8.21.0",
		"@rollup/plugin-babel": "^5.2.0",
		"@rollup/plugin-commonjs": "^15.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-replace": "^2.3.3",
		"@rollup/plugin-typescript": "^5.0.2",
		"@types/bent": "^7.3.0",
		"@types/child-process-promise": "^2.2.1",
		"@types/fs-extra": "^9.0.1",
		"@types/inquirer": "^7.3.1",
		"@types/jest": "^26.0.10",
		"@types/lodash": "^4.14.160",
		"@types/node": "^14.6.2",
		"@types/prettier": "^2.1.0",
		"@types/tar": "^4.0.3",
		"@types/tar-stream": "^2.1.0",
		"@types/yargs": "^15.0.5",
		"@typescript-eslint/eslint-plugin": "^3.10.1",
		"@typescript-eslint/parser": "^3.10.1",
		"apollo": "^2.30.3",
		"babel-jest": "^26.3.0",
		"babel-plugin-lodash": "^3.3.4",
		"babel-plugin-macros": "^2.8.0",
		"bent": "^7.3.10",
		"builtin-modules": "^3.1.0",
		"chalk": "^4.1.0",
		"change-case": "^4.1.1",
		"child-process-promise": "^2.2.1",
		"dev-console.macro": "^0.1.0",
		"dotenv": "^8.2.0",
		"eslint": "^7.7.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-jest": "^23.20.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
		"graphql-tag.macro": "^2.0.1",
		"husky": "^4.2.5",
		"inquirer": "^7.3.3",
		"jest": "^26.4.2",
		"lint-staged": "^10.2.13",
		"lodash": "^4.17.20",
		"prettier": "^2.1.1",
		"rollup": "^2.26.7",
		"rollup-plugin-dts": "^1.4.12",
		"rollup-plugin-license": "^2.2.0",
		"rollup-plugin-terser": "^7.0.1",
		"source-map-support": "^0.5.19",
		"tar": "^6.0.5",
		"ts-babel-node": "^1.1.1",
		"ts-jest": "^26.3.0",
		"ts-node": "^9.0.0",
		"tsconfig-paths": "^3.9.0",
		"tslib": "^2.0.1",
		"typescript": "^4.0.2",
		"yargs": "^15.4.1"
	},
	"dependencies": {
		"@actions/core": "^1.2.5",
		"@actions/github": "^4.0.0",
		"@apollo/client": "^3.1.4",
		"cross-fetch": "^3.0.5",
		"fs-extra": "^9.0.0",
		"graphql": "^15.3.0",
		"gunzip-maybe": "^1.4.2",
		"jszip": "^3.5.0",
		"semver": "^7.3.2",
		"subscriptions-transport-ws": "^0.9.18",
		"tar-stream": "^2.1.3",
		"typed-rest-client": "^1.7.3",
		"whatwg-url": "^8.2.1"
	}
}
